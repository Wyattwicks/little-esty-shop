<%= render partial: 'admin/header'%>

<div class='admin_dashboard_body'>
  <div id="incomplete_invoices">
    <h3 class="center">Incomplete Invoices</h3>
    <% @invoices.where_not_successful.each do |invoice| %>
      <p>Invoice #<%= link_to "#{invoice.id}", admin_invoice_path(invoice) %> - <%= invoice.created_at.strftime("%A, %B %d, %Y") %></p>
    <% end %>
  </div>

  <div>
    <h3>Top Customers</h3>

  </div>
</div>